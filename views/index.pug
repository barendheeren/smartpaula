extends portal.pug

block content
    div.row
        div.col-md-6.col-xs-12
            div.x-panel
                div.x-title
                    h2='Gebruiker'
                div.x-content
                form#demo-form(data-parsley-validate)
                    label(for="user")="Gebruiker"
                    select#user.form-control(required)
                        option(value="")="Kies..."
                        each user in users
                            option(value=user)=user
                    button.btn.btn-default(type=submit)=Submit